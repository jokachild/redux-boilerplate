{
  "name": "boilerplate",
  "version": "0.0.0",
  "description": "Redux boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack -d",
    "watch": "webpack -d --watch",
    "lint": "eslint ./src/js --ext .js --ext .jsx",
    "test": "mocha --compilers js:babel-core/register --recursive ./test/unit",
    "test:watch": "npm test -- --watch",
    "devtest": "webpack-dev-server --config webpack.test.config.js"
  },
  "keywords": [
    "redux",
    "react",
    "es6"
  ],
  "author": "Andrey Myroniuk",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jokachild/redux-boilerplate.git"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1"
  }
}
